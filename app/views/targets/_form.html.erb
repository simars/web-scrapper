    <%= f.text_area :description %>
  </div>

  <div class="field">
    <%= f.label :lead_source %><br />
    <%= f.text_field :lead_source %>
  </div>

  <div class="field">
    <%= f.label :industry %><br />
    <%= f.text_field :industry %>
  </div>

  <div class="field">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>

  <div class="field">
